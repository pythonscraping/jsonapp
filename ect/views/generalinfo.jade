head
	link(href='/background.css', rel='stylesheet')
	link(href='https://fonts.googleapis.com/css?family=Libre Franklin',rel='stylesheet')

	

div(class="login")
	div(class="login-form  container space-between")
		form(action="/generalinfo" method="post" class="container space-between")
			h5 Eclectic - General info
			div
				span You are a 
					select(name="gender")
						option(value="null") -
						option(value="female") Female
						option(value="male") Male
				br
			div
				span How old are you ? 
					select(name="age")
						option(value="null") -
						- var n = 18;
						while n < 50
							option(value=""+n)=n
							p=n++
				br
			span Looking for people between 
				select(name="minage")
					option(value="null") -
					- var n = 18;
					while n < 50
						option(value=""+n)=n
						p=n++
			br
			span and 
				select(name="maxage")
					option(value="null") -
					- var n = 18;
					while n < 50
						option(value=""+n)=n
						p=n++
			br
			br
			button(type="submit", class="asbestos-flat-button") Next 


